<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Config:etc/system_file.xsd">
    <system>
        <tab id="jenyamba" translate="label" sortOrder="10">
            <label>Jenyamba</label>
        </tab>
        <section id="cron_schedule" translate="label" sortOrder="130" showInDefault="1" showInWebsite="1"
                 showInStore="1">
            <class>separator-top</class>
            <label>Cron Configuration</label>
            <tab>jenyamba</tab>
            <resource>Jenyamba_ParseXml::cron_schedule</resource>
            <group id="general" translate="label" type="text" sortOrder="10" showInDefault="1" showInWebsite="0"
                   showInStore="0">
                <label>Cron Schedule</label>
                <field id="interval_cron" translate="label" type="text" sortOrder="1" showInDefault="1"
                       showInWebsite="0" showInStore="0">
                    <label>Interval cron</label>
                </field>
            </group>
        </section>
    </system>
</config>
